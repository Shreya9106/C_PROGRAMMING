//without parameter without return

#include <stdio.h>
void add()
{
    int a=10, b=5;
    printf("%d\n", (a+b));
}
void main()
{

    add();
}

Output: 15

------------------------------------------------------------------------------------------------------------------------------------------------------------

//Call by value

#include<stdio.h>
void swap(int,int);
void main()
{
    int a=10, b=20;
    printf("Before swapping: a = %d, b = %d \n",a,b);
    
    swap(a,b);
    
    printf("After swapping: a = %d, b = %d",a,b);

}
void swap(int x, int y)
{
    int temp;
    temp=x;
    x=y;
    y=temp;
}

Sample Output:
Before swapping: a = 10, b = 20 
After swapping: a = 10, b = 20
